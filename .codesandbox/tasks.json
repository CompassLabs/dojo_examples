{
  "setupTasks": [
    {
      "name": "Installing hardhat",
      "command": "npm install --save-dev hardhat"
    },
    {
      "name": "Downloading pip",
      "command": "wget https://bootstrap.pypa.io/pip/pip.pyz -O /tmp/pip.pyz"
    },
    {
      "name": "Installing dojo",
      "command": "python3.9 /tmp/pip.pyz install dojo-compass"
    }
  ],
  "tasks": {
    "start-app": {
      "name": "Run dashboard",
      "command": "python3.9 start_dashboard.py",
      "runAtStart": true
    }
  }
}
